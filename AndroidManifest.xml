<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidfactorem.jasonisframed"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name="com.androidfactorem.jasonisframed.MainActivity"
            android:label="@string/title_bar_main"
            android:uiOptions="splitActionBarWhenNarrow" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            
        </activity>
        <activity
      android:name="com.androidfactorem.jasonisframed.ViewWeb"></activity>
    <provider android:name="com.androidfactorem.jasonisframed.JSONDBProvider"
          android:authorities="com.androidfactorem.jsondbprovider" />    
    </application>

</manifest>